{
  "name": "just-another-xml2json",
  "version": "0.3.0",
  "description": "Just an another XML to JSON (and back) converter",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist coverage",
    "dev": "node --loader ts-node/esm src/index.ts",
    "build": "rollup -c",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "prepack": "npm run build"
  },
  "keywords": [
    "xml",
    "json",
    "xml2json",
    "json2xml",
    "xsd",
    "schema"
  ],
  "author": "Gyula Horvath <gylhrvth@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "dependency-cruiser": "^17.3.2",
    "jest": "^30.2.0",
    "rimraf": "^6.1.2",
    "rollup": "^4.53.3",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist/",
    "lib/"
  ]
}